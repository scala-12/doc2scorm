<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:t='urn:import:com.github.gwtbootstrap.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel>
		<t:Modal ui:field="lockScreen">
			<t:Well>Processing request in progress. Please, wait...</t:Well>
			<t:ProgressBar visible="true" type="ANIMATED" color="DEFAULT"
				percent="100" />
		</t:Modal>
		<t:Form styleName="form-horizontal" action="" method="get">
			<t:ControlGroup>
				<t:ControlLabel>Make a choice:</t:ControlLabel>
				<t:Controls ui:field="radioGroup"></t:Controls>
			</t:ControlGroup>
			<t:ControlGroup>
				<g:HTMLPanel>
					<t:Controls>
						<g:Image ui:field="paypalButton"
							url="https://www.paypal.com/en_US/i/btn/btn_xpressCheckout.gif"></g:Image>
					</t:Controls>
				</g:HTMLPanel>
			</t:ControlGroup>
		</t:Form>
	</g:HTMLPanel>

</ui:UiBinder>
